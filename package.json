{
	"name": "vite5-vue",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"prod": "vite --mode production",
		"build:dev": "vue-tsc && vite build --mode development",
		"build:prod": "vue-tsc && vite build --mode production",
		"preview": "vite preview",
