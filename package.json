		"vue-router": "4"
	},
	"devDependencies": {
		"@types/node": "^20.9.4",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"@vitejs/plugin-vue": "^4.5.0",
		"ant-design-vue": "4.0.7",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-vue": "^9.18.1",
		"prettier": "^3.1.0",
		"sass": "^1.69.5",
		"typescript": "^5.2.2",
